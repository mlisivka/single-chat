<div id="all_users">
<% @users.each do |user| %>
	<div class="user">
		<% if user.online? %>
			<div class="row user-status online">
		<% else %>
			<div class="row user-status offline">
		<% end %>
		<div class="user-email"><%= user.email %></div>
	</div>
	<div class="row btn-chat"><%= link_to 'open chat', user %></div>
</div>
<% end %>
</div>